<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DP2_Auto_App.Contents.PasswordPage"
             Title="Cambio de contraseña">
    <ContentPage.Content>
        <StackLayout>
            <Entry Text="Ingrese contraseña anterior"></Entry>
            <Entry Text="Ingrese nueva contraseña"></Entry>
            <Entry Text="Vuelva a ingresar la nueva contraseña"></Entry>
            <Button Text="Aceptar"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>