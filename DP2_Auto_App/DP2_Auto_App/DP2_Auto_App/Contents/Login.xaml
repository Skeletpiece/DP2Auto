<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DP2_Auto_App.Contents.Login">
    <ContentPage.Content>
        <StackLayout VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand">
            <Entry x:Name="label_Username" Placeholder="Usuario@DP2.com" Text="jarias@pucp.edu.pe"/>
            <Entry x:Name="label_Password" Placeholder="Password" IsPassword="True" Text="123456"/>
            <Button x:Name="button_SignIn" Text="--->" Clicked="button_SignIn_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>