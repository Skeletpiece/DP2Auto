<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DP2_Auto_App.Contents.GoalsPage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Logros y objetivos"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            <Button Text="Objetivos logrados" HorizontalOptions="Center" Margin="0, 0, 0, 15"/>
            <Button Text="Objetivos trazados" HorizontalOptions="Center" Margin="0, 0, 0, 15"/>
            <Button Text="Nuevo objetivo" HorizontalOptions="Center" Margin="0, 0, 0, 15"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>